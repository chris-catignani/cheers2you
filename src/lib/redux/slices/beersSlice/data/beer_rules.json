{
    "brewery": {
        "wordsToTrim": [
            "Brewing Company",
            "Brewing Co.",
            "Craft Brew Co.",
            "Craft Brew Co",
            "Brewing Co",
            "Beer Co.",
            "Beer Co",
            "Brew Co.",
            "Brew Co",
            "Breweries",
            "Brewery Co",
            "Brewery",
            "Brewing",
            "Craft Brews",
            "Craft Brew",
            "Brews",
            "Brauerei",
            "-Brauerei",
            "Weissbr√§u",
            "Biers de",
            "Brasserie des",
            "Brasserie de",
            "Brouwerij der Trappisten van",
            "Brouwerij Van",
            "Brouwerij",
            "Bierbrouwerij",
            "B.V.",
            "& Sohn",
            "Bryggeri"
        ],
        "regexes": [
            {
                "decription": "Remove '- Huyghe' from the brewery: Delirium - Huyghe",
                "regex": "Delirium - Huyghe",
                "replacement": "Delirium"
            }
        ]
    },
    "beerName": {
        "wordsToTrim": [
            "Barrel Aged Nitro"
        ],
        "wordsToKeepCapitalized": [
            "IPA",
            "DIPA",
            "AIPA",
            "EPA",
            "APA",
            "ESB",
            "NE",
            "NEIPA",
            "NEPA",
            "BA",
            "BPA",
            "PA",
            "XPA",
            "NZ"
        ],
        "regexes": [
            {
                "decription": "Remove years in parenthesis, e.g (2004)",
                "regex": "\\(\\d{4,}\\)",
                "replacement": ""
            },
            {
                "decription": "Remove range of years in parenthesis, e.g (2004-2005)",
                "regex": "\\(\\d{4,}\\w*-\\w*\\d{4,}\\)",
                "replacement": ""
            },
            {
                "decription": "Remove years if in 20XX",
                "regex": "\\s+20\\d{2,}\\s*",
                "replacement": ""
            },
            {
                "decription": "Remove anything in any type of parentheses",
                "regex": "/\\s*[\\[{(].*?[)}\\]]\\s*/g, ''",
                "replacement": ""
            }
        ]
    },
    "beerType": {
        "wordsToTrim": [
            "Non-Alcoholic Beer - "
        ],
        "regexes": [
            {
                "decription": "Remove the last '-' and everything after it.  Example 1: 'IPA - American' becomes 'IPA'.  Example 2: 'Semi-Sweet Wine - Other' becomes 'Semi-Sweet Wine'",
                "regex": "(.*?)-[^-]*$",
                "replacement": "$1"
            },
            {
                "regex": "Belgian Strong Dark Ale",
                "replacement": "Belgian Ale"
            }
        ]
    }
}
